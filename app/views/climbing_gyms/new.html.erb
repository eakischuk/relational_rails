<%= form_with url: "/climbing_gyms", method: :post, local: true do |f| %>

  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <%= f.radio_button :ropes_available, 'true' %>
  <%= f.label :ropes_available_true, 'Ropes' %>
  <%= f.radio_button :ropes_available, 'false' %>
  <%= f.label :ropes_available_false, 'No Ropes' %><br>
  <%= f.label :total_routes %><br>
  <%= f.number_field :total_routes %><br>
  <%= f.submit "Create Climbing Gym" %>
<% end %>
