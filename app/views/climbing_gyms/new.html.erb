<form action="/climbing_gyms" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter a new Climbing Gym: </p>
  <label for="name">Gym Name:</label><br>
  <input type="text" id="name" name="name"><br>
  <p>Ropes available?</p>
  <input type="radio" id="ropes" value="true" name="ropes_available">
  <label for="ropes">True</label><br>
  <input type="radio" id="no_ropes" value="false" name="ropes_available">
  <label for="no_ropes">False</label><br>
  <label for="route_count">Total Routes:</label><br>
  <input type="text" id="route_count" name="total_routes"><br>
  <input type="submit" value="Create Climbing Gym"
</form>
