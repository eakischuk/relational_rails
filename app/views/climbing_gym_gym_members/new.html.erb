<%= form_with url:"/climbing_gyms/#{@gym.id}/gym_members", method: :post, local: true do |f| %>
   <%= f.label :first_name %><br>
   <%= f.text_field :first_name %><br>
   <%= f.label :last_name %><br>
   <%= f.text_field :last_name %><br>
   <%= f.radio_button :belay_status, 'true' %>
   <%= f.label :belay_status_true, 'Belay Certified' %>
   <%= f.radio_button :belay_status, 'false' %>
   <%= f.label :belay_status_false, 'Not Belay Certified' %><br>
   <%= f.label :monthly_checkins %><br>
   <%= f.number_field :monthly_checkins %><br>
   <%= f.submit "Create Gym Member" %>
<% end %>
